#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>

using namespace std;

int main()
{
  int n = 1;
  int i = 1;
  int j = 1;
  int k = 100;
  
  while(n++ <= 6){
    
    for(; i <= 100; i++)
        if(i%15==0){
           cout << setw(3)<<left<< i << " ";
           i++;
           break;}
      
    for(; j <= 30; j++)
        if(j%5==0){
           cout <<setw(3)<<left<< j << " ";
           j++;
           break;}
           
     for(; k >= 50; k--)
        if(k%10==0){
           cout <<setw(3)<<left<< k << " ";
           k--;
           break;}
        
    cout << setw(3)<< left << pow(2,n) << " ";   

           
      cout << "\n";
  }     
}
