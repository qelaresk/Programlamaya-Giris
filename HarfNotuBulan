#include <iostream>
#include <string>

using namespace std;

char InttoChar(int a){
     
      if(a >= 90 && a <= 100)
         return 'A';
         else if(a >= 80 && a < 90)
                 return 'B';
          else if(a >= 70 && a < 80 )
                  return 'C';
             else 
                 return 'F';
                 
}



int main()
{
  int i; 
  cout<< "notunuzu giriniz: " << endl;
  cin>>i;
  
  cout << " harf notu: " << InttoChar(i);
  
  
  
}
