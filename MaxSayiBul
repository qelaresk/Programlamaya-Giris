#include <iostream>
#include <string>

using namespace std;

int max_func(int a, int b, int c){
      
      int max = a;
      
      if (max < b)
          max = b;
      if(max < c)
         max = c;
    return max;
}

int main()
{
  int i,j,k; 
  cout<< "uc sayi giriniz: " << endl;
  cin>>i>>j>>k;
  
  cout << "en buyuk sayi:" << max_func(i,j,k) << endl;
 



}
